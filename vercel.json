{
  "version": 2,
  "builds": [
    {
      "src": "data/api/*.json",
      "use": "@vercel/node"
    },
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/data/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.js"
    }
  ]
}
